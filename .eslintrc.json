{
    "env": {
        "browser": true,
        "jquery": true
    },
    "globals": {
        "TheShodo": true,
        "Kazari": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 5
    },
    "rules": {
        "no-unused-vars": "off",
        "no-console": "off",
        "no-redeclare": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-constant-condition": "off",
        "indent": "off",
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": "off",
        "semi": "off"
    }
}